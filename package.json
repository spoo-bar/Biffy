{
    "name": "biffy",
    "displayName": "Biffy",
    "description": "Language tools for BIF",
    "publisher": "spoorthi",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "BIF", "beml", "bxml"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/spoon611/Biffy.git"    
    },
    "icon": "images/icon.png",
    "contributes": {
        "languages": [{
            "id": "bif",
            "aliases": ["BIF", "bif"],
            "extensions": [".beml",".bxml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bif",
            "scopeName": "source.bif",
            "path": "./syntaxes/bif.tmLanguage.json"
        }],
        "snippets": [{
				"language": "bif",
				"path": "./snippets.json"
		}]
    }
}